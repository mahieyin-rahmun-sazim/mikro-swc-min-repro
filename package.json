{
  "name": "project-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.11"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e ./.env.development.local -- nest start --watch",
    "start:debug": "dotenv -e ./.env.development.local -- nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e ./.env.test.local -- jest --config jest-unit.config.json",
    "db:migration:create": "rm ./src/db/migrations/.snapshot.json || dotenv -e ./.env.development.local -- mikro-orm migration:create",
    "db:migration:up": "dotenv -e ./.env.development.local -- mikro-orm migration:up",
    "db:migration:down": "dotenv -e ./.env.development.local -- mikro-orm migration:down",
    "db:migration:list": "dotenv -e ./.env.development.local -- mikro-orm migration:list",
    "db:migration:check": "dotenv -e ./.env.development.local -- mikro-orm migration:check",
    "db:migration:pending": "dotenv -e ./.env.development.local -- mikro-orm migration:pending",
    "db:migration:fresh:dev": "dotenv -e ./.env.development.local -- mikro-orm migration:fresh",
    "db:migration:fresh:test": "dotenv -e ./.env.test.local -- mikro-orm migration:fresh",
    "test:watch": "dotenv -e ./.env.test.local -- jest --config jest-unit.config.json --watch",
    "test:cov": "dotenv -e ./.env.test.local -- jest --config jest-unit.config.json --coverage",
    "test:debug": "dotenv -e ./.env.test.local -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e ./.env.test.local -- jest --config jest-e2e.config.json"
  },
  "dependencies": {
    "@mikro-orm/cli": "^6.1.12",
    "@mikro-orm/core": "^6.1.12",
    "@mikro-orm/migrations": "^6.1.12",
    "@mikro-orm/nestjs": "^5.2.3",
    "@mikro-orm/postgresql": "^6.1.12",
    "@mikro-orm/seeder": "^6.1.12",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.7",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^10.3.7",
    "argon2": "^0.40.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.12",
    "@swc/jest": "^0.2.36",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/db/db.config.ts"
    ]
  }
}
