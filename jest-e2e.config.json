{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "maxWorkers": 1,
  "transform": {
    "^.+\\.(t|j)s$": ["@swc/jest"]
  },
  "moduleNameMapper": {
    "^@test/(.*)$": "<rootDir>/e2e/$1",
    "^@/(.*)$": "<rootDir>/src/$1"
  }
}
